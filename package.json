{
    "name": "new-san-cai",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "lint": "eslint src server --ext .js,.jsx",
        "lint:fix": "eslint src server --ext .js,.jsx --fix",
        "dev": "next dev",
        "server": "cross-env NODE_ENV=development nodemon server/babel.js nextRun=false",
        "seeds": "node server/seeds",
        "sql": "nodemon sqlTransformer/babel.js",
        "sql:prod": "pm2 delete sql-transform; pm2 start sqlTransformer/babel.js --name sql-transform --no-autorestart",
        "dump:make": "node server/helpers/backup/makeBackup.js",
        "dump:restore": "node server/helpers/backup/restoreBakup.js",
        "build": "next build",
        "start": "next start",
        "start:pm2:server": "cross-env NODE_ENV=production pm2 start --name new-san-cai server/babel.js --node-args=\"\" -- nextRun=false",
        "start:pm2:server:next": "cross-env NODE_ENV=production pm2 --name new-san-cai start server/babel.js --node-args=\"\" -- nextRun=true",
        "stop:pm2:server": "pm2 delete new-san-cai",
        "deploy": "npm run stop:pm2:server; cross-env npm run start:pm2:server && npm run build && npm run stop:pm2:server && npm run start:pm2:server:next"
    },
